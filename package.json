{
  "name": "hovet-ufrpe",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:igor-felipe/hovet-ufrpe.git",
  "author": "Igor Felipe",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "nodemon --exec sucrase-node src/index.ts",
    "test": "jest && yarn test:integration",
    "test:watch": "jest --watch --clearMocks",
    "test:integration": "jest --passWithNoTests --config jest.config.integration.js",
    "test:integration:watch": " yarn test:integration --watch",
    "lint": "eslint . --ext js,ts",
    "lint:fix": "eslint --fix . --ext js,ts",
    "lint:fix:cache": "eslint --fix --cache --ext js,ts",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "prepare": "husky install",
    "build": "tsc",
    "ci": "yarn lint && yarn type-check && yarn test",
    "commit": "git-cz"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "argon2": "^0.30.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fp-ts": "^2.16.0",
    "helmet": "^7.0.0",
    "isemail": "^3.2.0",
    "jose": "^4.14.4",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "prisma": "^5.0.0",
    "ramda": "^0.29.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.0",
    "@types/ramda": "^0.29.2",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "prettier": "3.0.0",
    "sucrase": "^3.32.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
